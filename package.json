{
  "name": "@flowwishthebest/tfp",
  "version": "0.0.1",
  "description": "Theoretical foundations of programming",
  "main": "dist/main.js",
  "directories": {
    "doc": "docs"
  },
  "bin": {
    "ilya": "./bin/main.js"
  },
  "scripts": {
    "test": "jest -c ./jest.config.json",
    "coverage": "cat ./coverage/lcov.info | coveralls",
    "build": "tsc",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "tslint --fix -p ./tsconfig.json -c ./tslint.json",
    "prettify": "prettier --config ./prettierrc.json --write '{src,__tests__}/**/*.ts'",
    "start": "node dist/main.js",
    "debug": "node --inspect-brk=0.0.0.0 dist/main.js"
  },
  "keywords": [
    "computer science",
    "cs",
    "interpreter",
    "translator"
  ],
  "author": "Ilya P. <togowiththeflow@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/node": "13.7.1",
    "@types/yargs": "^15.0.4",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "coveralls": "3.0.7",
    "eslint": "6.8.0",
    "eslint-plugin-jest": "^23.7.0",
    "handlebars": "^4.5.0",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "yargs": "^15.1.0"
  }
}
